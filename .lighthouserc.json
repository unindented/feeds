{
  "ci": {
    "collect": {
      "numberOfRuns": 2,
      "staticDistDir": "website/dist/",
      "url": ["/", "/feeds/krebsonsecurity-com/"]
    },
    "assert": {
      "preset": "lighthouse:recommended",
      "assertions": {
        "color-contrast": "off",
        "render-blocking-resources": "off"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
